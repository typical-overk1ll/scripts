loadstring(game:HttpGet("https://raw.githubusercontent.com/typical-overk1ll/scripts/main/Invite",true))()local a=game:GetService("Players").LocalPlayer;local b=loadstring(game:HttpGet("https://raw.githubusercontent.com/typical-overk1ll/scripts/main/UILibrary",true))()local c=workspace:WaitForChild("SessionVehicles")local d=b.new("overk1ll's Tuning Garage")local e=d:addPage("Engine Tuning")local f=d:addPage("Boost Tuning")local g=200;local h=0;local i=0;local j=false;local k=e:addSection("Tuning")k:addSlider("Horse Power",0,0,1500,function(l)g=l end)k:addButton("With great power comes great responsibility.")local m=f:addSection("Super Charger")local n=f:addSection("Turbo Charger")local o=d:addPage("Modifications"):addSection("Miscellaneous Mods")n:addSlider("Turbo Chargers",0,0,4,function(l)h=l end)m:addSlider("Super Chargers",0,0,2,function(l)i=l end)o:addToggle("Upgraded Brakes",false,function(l)j=l end)while wait()do pcall(function()local p=require(c:FindFirstChild(a.Name.."-Car"):FindFirstChild("A-Chassis Tune"))p.Horsepower=g;p.ClutchTol=10000;p.Redline=10000;p.TurboEnabled=true;p.Turbochargers=h;p.Superchargers=i;p.Weight=500;if j then local p=require(c:FindFirstChild(a.Name.."-Car"):FindFirstChild("A-Chassis Tune"))p.BrakeForce=10000000000000;p.PBrakeForce=10000000000000 end end)end
